<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.elanilsondejesus.com.controledopeso">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.PerfilActivity"></activity>
        <activity android:name=".activity.HistoricoActivity" />
        <activity android:name=".activity.EstartisticaActivity" />
        <activity
            android:name=".activity.MetaActivity"
            android:parentActivityName=".activity.HomeActivity" />
        <activity
            android:name=".activity.ProgressoActivity"
            android:parentActivityName=".activity.HomeActivity" />
        <activity
            android:name=".activity.InformacoesActivity"
            android:parentActivityName=".activity.HomeActivity" />
        <activity android:name=".activity.HomeActivity" />
        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>